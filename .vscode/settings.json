{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.customData": [".vscode/css_custom_data.json"],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.includeLanguages": {
    "css": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    ["@apply\\s+([^;}]+)", "([^;}]+)"],
    ["@layer\\s+([^;}]+)", "([^;}]+)"],
    ["@theme\\s+([^;}]+)", "([^;}]+)"]
  ],
  "emmet.includeLanguages": {
    "css": "css"
  }
}
